    def set_appear(self):
        self.setWindowTitle(txt_title)
        self.resize(win_w, win_h)
        self.move(win_x, win_y)
    def initUI(self):
        pass
    def connects(self):
        pass


app = QApplication([])
mw = MainWin()
app.exec_()